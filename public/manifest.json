{
  "manifest_version": 2,
  "name": "octodirect",
  "version": "0.3.0",
  "description": "Move to GitHub repository directly",
  "browser_action": {
    "default_icon": "octodirect_48.png",
    "default_popup": "index.html"
  },
  "default_locale": "en",
  "icons": {
    "16": "octodirect_16.png",
    "48": "octodirect_48.png",
    "128": "octodirect_128.png"
  },
  "homepage_url": "https://github.com/JaeYeopHan/octodirect",
  "permissions": ["activeTab", "tabs", "history", "storage"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+K",
        "linux": "Alt+Shift+K",
        "chromeos": "Alt+Shift+K",
        "mac": "Command+Shift+K",
        "default": "Ctrl+Shift+K"
      },
      "description": "open_popup"
    }
  }
}
