{
  "manifest_version": 2,

  "name": "Octodirect",
  "description": "Move to GitHub repository directly",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "octodirect_icon.png",
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "open_popup",
      "global": true
    }
  }
}
