# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

[{package.json,.travis.yml,*.css,*.scss}]
indent_style = space
indent_size = 2
insert_final_newline = false
