{
  "name": "octodirect",
  "author": "Jbee[ljyhanll@gmail.com]",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "axios": "^0.18.0",
    "evergreen-ui": "^3.2.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "^2.17.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "scripts": {
    "deploy": "npm-run-all versioning clean:zip build zipify dashboard",
    "clean:zip": "rm -rf octodirect.zip",
    "zipify": "zip -r octodirect.zip build",
    "versioning": "node ./scripts/deploy.js",
    "dashboard": "open https://chrome.google.com/webstore/developer/dashboard",
    "prettier:manifest": "prettier --write ./public/manifest.json",
    "typesync": "npx typesync && npm install",
    "coveralls": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "test:watch": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts --watch",
    "eject": "react-app-rewired eject --scripts-version react-scripts-ts"
  },
  "devDependencies": {
    "@types/chrome": "0.0.72",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-test-renderer": "^16.0.2",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-saga": "^0.10.5",
    "coveralls": "^3.0.2",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "inquirer": "^6.1.0",
    "jest": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "react-app-rewired": "^1.5.2",
    "react-test-renderer": "^16.4.2",
    "signale": "^1.2.1",
    "string-replace-webpack-plugin": "^0.1.3",
    "styled-components": "^3.4.2",
    "ts-jest": "^22.4.2",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.1",
    "typescript-styled-plugin": "^0.10.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "jest": {
    "testMatch": [
      "**/tests/unit/**/*.test.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
